#!/usr/bin/env php
<?php

declare(strict_types=1);

$file_local = __DIR__ . '/../../vendor/autoload.php';
$file_global = __DIR__ . '/../../../../autoload.php';

if (file_exists($file_local)) {
    require $file_local;
} else {
    require $file_global;
}

use function Renakdup\inc\CommandLine\setup;

setup();
